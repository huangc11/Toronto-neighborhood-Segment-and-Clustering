    "\n",
    "#Read html \n",
    "soup = BeautifulSoup(page.content, 'html.parser')\n",
    "\n",
    "#parse and convert to table\n",
    "table = soup.find_all('table')\n",
    "df_nbrhood = pd.read_html(str(table))[0]\n"
   ]
